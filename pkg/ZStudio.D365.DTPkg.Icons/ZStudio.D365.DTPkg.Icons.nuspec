<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>ZD365.DeploymentTool.Icons</id>
		<title>ZD365.DeploymentTool.Icons</title>
		<version>$version$</version>
		<authors>Winson Choo</authors>
		<owners>Winson Choo</owners>
		<!--<licenseUrl>http://www.zerostudioinc.com/</licenseUrl>
		<projectUrl>http://www.zerostudioinc.com/</projectUrl>-->
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>A library of icons (legacy 16x16, legacy 32x32 and SVG - vector for unified interface) that can be used for custom D365 CE entities. SVG Icons made by www.flaticon.com/authors/freepik from www.flaticon.com. The icons will be install to ZD365DT-Icons folder of the .NET Framework project when the NuGet is install. NOTE: NuGet content files copy is not supported on a .NET Core project.</description>
	</metadata>
	<files>
		<!--<file src="bin\$configuration$\netcoreapp3.1\ZStudio.D365.DTPkg.Icons.*" target="lib\netcoreapp3.1" />-->
		<file src="ZD365DT-Icons\legacy\16x16\*.*" target="content\ZD365DT-Icons\legacy\16x16" />
		<file src="ZD365DT-Icons\legacy\32x32\*.*" target="content\ZD365DT-Icons\legacy\32x32" />
		<file src="ZD365DT-Icons\svg\*.svg" target="content\ZD365DT-Icons\svg" />
	</files>
</package>