		<file src="ZD365DT-Icons\legacy\32x32\*.*" target="content\ZD365DT-Icons\legacy\32x32" />
		<file src="ZD365DT-Icons\svg\*.svg" target="content\ZD365DT-Icons\svg" />
	</files>
</package>