<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>ZD365.DeploymentTool.Icons</id>
		<title>ZD365.DeploymentTool.Icons</title>
		<version>$version$</version>
		<authors>Winson C</authors>
		<license type="expression">MIT</license>
		<!--<licenseUrl>https://github.com/zerox2cool/d365dt/blob/main/LICENSE</licenseUrl>-->
